<br>
<div class="container">
  <br>
  <h2>My Listings</h2>
  <div class="row">
    <div class="col-md-12">
      <table class="table table-striped">
        
        <thead>
          <tr>
            <th>Brand</th>
            <th>Model</th>
            <th>Price</th>
            <th>Condition</th>
            <th>Start time</th>
            <th>End time</th>
            <th>Status</th>
            <th>Owner Email</th>
            <th colspan="3"></th>
          </tr>
        </thead>

        <tbody>
          <% @listings.each do |listing| %>
          <tr>
            <td><%= listing.brand %></td>
            <td><%= listing.model %></td>
            <td><%= listing.price %></td>
            <td><%= listing.condition %></td>
            <td><%= listing.start_time %></td>
            <td><%= listing.end_time %></td>
            <td><%= listing.status %></td>
            <td><%= current_user.email %></td>
            <td><%= link_to 'Show', listing %></td>
            <td><%= link_to 'Edit', edit_listing_path(listing) %></td>
            <td><%= link_to 'Delete', listing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
          <% end %>
       </tbody>

      </table>
    </div>
  </div>
</div>
